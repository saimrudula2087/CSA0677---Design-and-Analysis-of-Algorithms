#include <stdio.h>
void generateMaxLocal(int grid[][4], int n, int maxLocal[][2]) {
    for (int i = 0; i < n - 2; i++) {
        for (int j = 0; j < n - 2; j++) {
            int maxVal = grid[i][j];
            for (int k = i; k < i + 3; k++) {
                for (int l = j; l < j + 3; l++) {
                    if (grid[k][l] > maxVal) {
                        maxVal = grid[k][l];
                    }
                }
            }
            maxLocal[i][j] = maxVal;
        }
    }
}

int main() {
    int grid[4][4] = {
        {9, 9, 8, 1},
        {5, 6, 2, 6},
        {8, 2, 6, 4},
        {6, 2, 2, 2}
    };
    int n = 4; // Size of grid
    int maxLocal[2][2];

    generateMaxLocal(grid, n, maxLocal);

    printf("Output:\n");
    for (int i = 0; i < n - 2; i++) {
        for (int j = 0; j < n - 2; j++) {
            printf("%d ", maxLocal[i][j]);
        }
        printf("\n");
    }

    return 0;
}
