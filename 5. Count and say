#include<stdio.h>
int add(int n);
int main()
{
    int n;
    scanf("%d",&n);
    int res=add(n);
    printf("%d",res);
}
int add(int n)
{
    int m,temp,sum=0,count=0;
    while(n>0)
    {
        m=n%10;
        sum=sum+m;
        n=n/10;
        temp=m;
        if(temp==n)
        {
            count++;
        }
        sum=sum+count;
    }
    if(sum>0)
    {
        return sum;
    }
}

