#include <stdio.h>
int main() 
{
    int nums[] = {2, 7, 11, 15};
	int target = 9;
    int numsSize = sizeof(nums) / sizeof(nums[0]); 
    int result[2]; 
    for (int i = 0; i < numsSize; i++) 
	{
        for (int j = i + 1; j < numsSize; j++) 
		{
            if (nums[i] + nums[j] == target) 
			{
                result[0] = i;
                result[1] = j; 
                printf("Indices: [%d, %d]\n", result[0], result[1]);
                return 0; 
            }
        }
    }
    return 0;
}
