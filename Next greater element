#include <stdio.h>
void findSecondGreater(int nums[], int size) {
    int answer[size];
    
    for (int i = 0; i < size; i++) {
        int secondGreater = -1;
        for (int j = i + 1; j < size; j++) {
            if (nums[j] > nums[i]) {
                secondGreater = nums[j];
                break;
            }
        }
        answer[i] = secondGreater;
    }
    printf("Output: [");
    for (int i = 0; i < size; i++) {
        printf("%d", answer[i]);
        if (i < size - 1) {
            printf(",");
        }
    }
    printf("]\n");
}

int main() {
    int nums[] = {2, 4, 0, 9, 6};
    int size = sizeof(nums) / sizeof(nums[0]);
    
    findSecondGreater(nums, size);
    
    return 0;
}
