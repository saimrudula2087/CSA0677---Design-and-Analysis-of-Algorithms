#include<stdio.h>
int lis(int a[],int n,int target)
{
	for(int i=0;i<n;i++)
	{
		if(a[i]==target)
		{
			return i;
		}
	}
	return -1;
}
void bubblesort(int n,int a[])
{
	for(int i=0;i<n;i++)
	{
		for(int j=0;j<n;j++)
		{
			if(a[i]>a[j])
			{
				int temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
}
int main()
{
	int a[]={2,5,1,4,6};
	int target=4;
	int n=sizeof(a)/sizeof(a[0]);
	int res=lis(a,n,target);
	if(res==-1)
	{
		printf("%d is not found\n",target);
	}
	else 
	{
		printf("%d is found at %d",target,res);
	}
	bubblesort(n,a);
	for(int i=0;i<n;i++)
	{
		printf("%d ",a[i]);
	}
}
