#include <stdio.h>
#include <string.h>
void removeCharacters(char s[]) {
    for (char c = 'a'; c <= 'z'; c++) {
        int index = -1;
        for (int i = 0; i < strlen(s); i++) {
            if (s[i] == c) {
                index = i;
                break;
            }
        }
        if (index != -1) {
            // Shift subsequent characters to the left
            for (int i = index; i < strlen(s) - 1; i++) {
                s[i] = s[i + 1];
            }
            s[strlen(s) - 1] = '\0';
            index--;
        }
    }
}
int main() {
    char s[] = "aabcbbca";
    removeCharacters(s);
    printf("Resulting String: %s\n", s);
    return 0;
}
