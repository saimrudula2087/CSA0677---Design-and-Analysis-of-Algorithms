#include <stdio.h>

int isPrime(int num, int divisor);
int main() {
    int start, end;

    printf("Enter the starting and ending numbers of the range: ");
    scanf("%d %d", &start, &end);

    printf("Prime numbers in the range %d to %d are: ", start, end);
    printPrimesInRange(start, end);

    return 0;
}
int isPrime(int num, int divisor) {
    if (divisor == 1) {
        return 1;
    } else {
        if (num % divisor == 0 && divisor != num) {
            return 0;
        } else {
            return isPrime(num, divisor - 1);
        }
    }
}

void printPrimesInRange(int start, int end) {
    if (start <= end) {
        if (isPrime(start, start / 2)) {
            printf("%d ", start);
        }
        printPrimesInRange(start + 1, end);
    }
}
