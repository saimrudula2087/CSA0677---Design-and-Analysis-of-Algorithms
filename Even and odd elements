#include<stdio.h>
int main()
{
	int a[]={23,14,88,56,67,83,20,29};
	int n,i;
	n=sizeof(a)/sizeof(a[0]);
	printf("even elements: ");
	for(i=0;i<n;i++)
	{
		if(a[i]%2==0)
		{
			printf("%d ",a[i]);
		}
	}
	printf("\neven elements: ");
	for(i=0;i<n;i++)
	{
		if(a[i]%2!=0)
		{
			printf("%d ",a[i]);
		}
	}
}
