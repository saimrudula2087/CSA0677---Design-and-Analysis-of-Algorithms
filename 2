#include<stdio.h>
int main()
{
	int a[]={2,4,6,8,10,12,14,18};
	int n=sizeof(a)/sizeof(a[0]);
	int i,j,temp;
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(a[i]>a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	printf("Maximum element: %d",a[0]);
	printf("\nMinimum element: %d",a[n-1]);
}
