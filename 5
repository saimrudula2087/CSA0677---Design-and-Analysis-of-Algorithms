#include<stdio.h>
void selectionsort(int n,int a[])
{
	int i,j,temp;
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(a[i]<a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
	for(i=0;i<n;i++)
	{
		printf("%d ",a[i]);
	}
}
int main()
{
	int a[]={5,2,9,1,5,6};
	int n=sizeof(a)/sizeof(a[0]);
	selectionsort(n,a);
}
