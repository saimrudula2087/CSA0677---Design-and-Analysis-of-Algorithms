#include <stdio.h>
#include <string.h>
#define MAX_CHARS 256
int main() 
{
    char s[] = "pwwkew";
    int n = strlen(s);
    int max_len = 0, start = 0;
    int index[MAX_CHARS]; 
    for (int i = 0; i < MAX_CHARS; i++) 
	{
        index[i] = -1;
    }
    for (int end = 0; end < n; end++) 
	{
        if (index[s[end]] >= start) 
		{
            start = index[s[end]] + 1;
        }
        index[s[end]] = end;
        if (end - start + 1 > max_len) 
		{
            max_len = end - start + 1;
        }
    }
    printf("Input: s = \"%s\"\n", s);
    printf("Output: %d\n", max_len);
    return 0;
}
